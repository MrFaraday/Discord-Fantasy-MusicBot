{
    "name": "discord-fantasy-music-bot",
    "version": "1.4.0",
    "description": "Music bot fo fantasy board gaming",
    "main": "./build/index.js",
    "scripts": {
        "build": "rimraf ./build && tsc && npm run postbuild",
        "start": "node build/index.js",
        "dev": "nodemon",
        "lint": "eslint . --ext .ts",
        "postbuild": "cpy **/* !**/*.ts ../build --cwd=src --no-overwrite --parents"
    },
    "keywords": [
        "bot",
        "discord",
        "music",
        "fantasy"
    ],
    "author": "MrFaraday <faradayby@gmail.com>",
    "homepage": "https://github.com/mr-faraday/discord-fantasy-musicbot/#readme",
    "repository": "https://github.com/mr-faraday/discord-fantasy-musicbot.git",
    "license": "MIT",
    "dependencies": {
        "@discordjs/opus": "^0.5.0",
        "axios": "^0.21.1",
        "discord.js": "^12.5.3",
        "lodash.shuffle": "^4.2.0",
        "pg": "^8.5.1",
        "pg-format": "^1.0.4",
        "sodium": "^3.0.2",
        "ytdl-core-discord": "^1.3.0"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/lodash.shuffle": "^4.2.6",
        "@types/node": "^14.14.41",
        "@types/pg": "^7.14.11",
        "@types/pg-format": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "cpy-cli": "^3.1.1",
        "dotenv": "^8.2.0",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "prettier-eslint": "^12.0.0",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
